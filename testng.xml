        <classes>
            <!--  如果放在包里面一定要带包名，不然找不到这个用例-->
            <class name="com.carams.testcase.TestcreateVehicle_detail"/>

        </classes>

    </test>

    <test name="查询车辆接口用例">
    <classes>
        <!--  如果放在包里面一定要带包名，不然找不到这个用例-->
        <class name="com.carams.testcase.TestgetVehicleReadyRunList_detail"/>

    </classes>
    </test>





    <listeners>

        <listener class-name="org.uncommons.reportng.HTMLReporter" />

        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />

    </listeners>
</suite>